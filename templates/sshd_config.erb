<%- if @comment.length > 0 -%>
<%= @comment %>
<%- else -%>
# This file is being maintained by Puppet.
# DO NOT EDIT
<%- end -%>
<%- if @port.length > 0 -%>
<%- @port.each {|p| -%>
Port <%= p %>
<%- } -%>
<%- end -%>
<%- if @protocol -%>
Protocol <%= @protocol %>
<%- end -%>
<%- if @address_family -%>
AddressFamily <%= @address_family %>
<%- end -%>
<%- if @listen_addresses.length > 0 -%>
<%- @listen_addresses.each {|listen_address| -%>
ListenAddress <%= listen_address %>
<%- } -%>
<%- end -%>
<%- if @host_keys.length > 0 -%>
<%- @host_keys.each {|host_key| -%>
HostKey <%= host_key %>
<%- } -%>
<%- end -%>
<%- if @key_regeneration_interval -%>
KeyRegenerationInterval <%= key_regeneration_interval %>
<%- end -%>
<%- if @use_dns -%>
UseDNS yes
<%- else -%>
UseDNS no
<%- end -%>
<%- if @use_pam -%>
UsePAM yes
<%- else -%>
UsePAM no
<%- end -%>
<%- if @print_motd -%>
PrintMotd yes
<%- else -%>
PrintMotd no
<%- end -%>
<%- if @strict_modes -%>
StrictModes yes
<%- else -%>
StrictModes no
<%- end -%>
<%- if @allow_agent_forwarding -%>
AllowAgentForwarding yes
<%- else -%>
AllowAgentForwarding no
<%- end -%>
<%- if @allow_tcp_forwarding -%>
AllowTcpForwarding yes
<%- else -%>
AllowTcpForwarding no
<%- end -%>
<%- if @allow_stream_local_forwarding -%>
AllowStreamLocalForwarding yes
<%- else -%>
AllowStreamLocalForwarding no
<%- end -%>
<%- if @chroot_directory -%>
ChrootDirectory <%= @chroot_directory %>
<%- end -%>
<%- if @challenge_response_authentication -%>
ChallengeResponseAuthentication yes
<%- else -%>
ChallengeResponseAuthentication yes
<%- end -%>
<%- if @authorized_keys_file -%>
AuthorizedKeysFile <%= @authorized_keys_file.join(" ") %>
<%- end -%>
<%- if @compression -%>
Compression yes
<%- else -%>
Compression no
<%- end %>
Ciphers <%= @ciphers.join(",") %>
KexAlgorithms <%= @kex_algorithms.join(",") %>
Macs <%= @macs.join(",") %>
<%- if @disable_forwarding -%>
DisableForwarding <%= @disable_forwarding %>
<%- end -%>
<%- if @gssapi_authentication -%>
GSSAPIAuthentication yes
<%- else -%>
GSSAPIAuthentication no
<%- end -%>
<%- if @gssapi_cleanup_credentials -%>
GSSAPICleanupCredentials yes
<%- else -%>
GSSAPICleanupCredentials no
<%- end -%>
<%- if @log_level -%>
LogLevel <%= @log_level -%>
<%- end %>
<%- if @ignore_rhosts -%>
IgnoreRhosts yes
<%- else -%>
IgnoreRhosts no
<%- end -%>
<%- if @ignore_user_known_hosts -%>
IgnoreUserKnownHosts yes
<%- else -%>
IgnoreUserKnownHosts no
<%- end -%>
<%- if @login_grace_time -%>
LoginGraceTime <%= @login_grace_time %>
<%- end -%>
<%- case @permit_root_login; when true, "on" -%>
PermitRootLogin yes
<%- when false, "no" -%>
PermitRootLogin no
<%- else -%>
PermitRootLogin <%= @permit_root_login %>
<%- end -%>
<%- if @client_alive_count_max -%>
ClientAliveCountMax <%= @client_alive_count_max %>
<%- end -%>
<%- if @client_alive_interval -%>
ClientAliveInterval <%= @client_alive_interval %>
<%- end %>
<%- if @permit_empty_passwords -%>
PermitEmptyPasswords yes
<%- else -%>
PermitEmptyPasswords no
<%- end -%>
<%- if @password_authentication -%>
PasswordAuthentication yes
<%- else -%>
PasswordAuthentication no
<%- end -%>
<%- if @pubkey_authentication -%>
PubKeyAuthentication yes
<%- else -%>
PubKeyAuthentication no
<%- end -%>
<%- if @permit_tty -%>
PermitTTY yes
<%- else -%>
PermitTTY no
<%- end -%>
<%- if @permit_tunnel -%>
PermitTunnel yes
<%- else -%>
PermitTunnel no
<%- end -%>
<%- if @max_auth_tries -%>
MaxAuthTries <%= @max_auth_tries %>
<%- end -%>
<%- if @max_sessions -%>
MaxSessions <%= @max_sessions %>
<%- end -%>
<%- if @disable_forwarding -%>
DisableForwarding yes
<%- else -%>
DisableForwarding no
<%- end -%>
<%- if @banner -%>
Banner <%= @banner %>
<%- end -%>
<%- if @debian_banner -%>
DebianBanner yes
<%- else -%>
DebianBanner no
<%- end -%>
<%- if @tcp_keepalive -%>
TCPKeepAlive yes
<%- else -%>
TCPKeepAlive no
<%- end -%>
<%- if @x11_forwarding -%>
X11Forwarding yes
<%- else -%>
X11Forwarding no
<%- end -%>
<%- if @permit_user_rc -%>
PermitUserRC yes
<%- else -%>
PermitUserRC no
<%- end -%>
<%- if @x11_use_localhost -%>
X11UseLocalhost yes
<%- else -%>
X11UseLocalhost no
<%- end -%>
<%- if @permit_user_environment -%>
PermitUserEnvironment yes
<%- else -%>
PermitUserEnvironment no
<%- end -%>
<%- if @accept_env.length > 0 -%>
<%- @accept_env.each {|a| -%>
AcceptEnv <%= a %>
<%- } -%>
<%- end -%>
<%- if @deny_users.length > 0 -%>
<%- @deny_users.each {|u| -%>
DenyUsers <%= u %>
<%- } -%>
<%- end -%>
<%- if @allow_users.length > 0 -%>
<%- @allow_users.each {|u| -%>
AllowUsers <%= u %>
<%- } -%>
<%- end -%>
<%- if @deny_groups.length > 0 -%>
<%- @deny_groups.each {|g| -%>
DenyGroups <%= g %>
<%- } -%>
<%- end -%>
<%- if @allow_groups.length > 0 -%>
<%- @allow_groups.each {|g| -%>
AllowGroups <%= g %>
<%- } -%>
<%- end -%>
<%- if @fingerprint_hash -%>
FingerprintHash <%= @fingerprint_hash %>
<%- end -%>
<%- if @permit_open.length > 0 -%>
<%- @permit_open.each {|po| -%>
PermitOpen <%= po %>
<%- } -%>
<%- end -%>
<%- if @ipqos -%>
IPQoS <%= @ipqos.join(" ") %>
<%- end -%>
<%- if @host_based_accepted_key_types -%>
HostBasedAcceptedKeyTypes <%= @host_based_accepted_key_types.join(",") %>
<%- end -%>
<%- if @host_certificates.length > 0 -%>
<%- @host_certificates.each {|hc| -%>
HostCertificate <%= hc %>
<%- } -%>
<%- end -%>
